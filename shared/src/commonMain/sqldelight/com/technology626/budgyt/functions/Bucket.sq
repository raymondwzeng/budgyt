addBucket:
    INSERT INTO Bucket(id, bucket_name, bucket_type, bucket_estimate)
    VALUES (?, ?, ?, ?);

getBuckets:
    SELECT id, bucket_name, bucket_type, bucket_estimate
    FROM Bucket;

editBucket:
    UPDATE Bucket
    SET bucket_name = ?, bucket_type = ?, bucket_estimate = ?
    WHERE id = ?;